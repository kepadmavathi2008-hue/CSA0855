a=int(input("enter a:\n"))
b=int(input("enter b:\n"))
for num in range(a,b+1):
    if num>1:
        count=0
        for i in range(1,num+1):
            if num%i==0:
                count=count+1
        if count==2:
            print("prime number:",num)

output:
prime number: 2
prime number: 3
prime number: 5
prime number: 7
